[Unit]
Description=Gunicorn instance to serve myproject
After=network.target

[Service]
User=juadel
Group=www-data
WorkingDirectory=/home/juadel/app
Environment="PATH=/home/juadel/app/env/bin"
ExecStart=/home/juadel/app/env/bin/gunicorn --workers 3 --bind unix:app.sock -m 007 wsgi:app

[Install]
WantedBy=multi-user.target